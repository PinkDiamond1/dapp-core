{
  "name": "@trustless-computer/dapp-core",
  "version": "0.0.2",
  "description": "Codebase for Trustless Computer DApps",
  "author": "benjiro2077",
  "license": "Apache-2.0",
  "repository": "@trustless-computer/dapp-core",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rm -rf dist & rollup -c --bundleConfigAsCjs",
    "start": "rollup -c -w --bundleConfigAsCjs",
    "lint:js": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:css": "stylelint --fix 'src/**/*.{scss,css}'",
    "lint:json": "prettier --write \"./**/*.json\"",
    "prepare": "husky install"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^3.20.3",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sass": "^1.12.19",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-uglify": "^6.0.4",
    "stylelint": "^15.5.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ],
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.0.2",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/keccak256": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "bignumber.js": "^9.1.1",
    "bip32": "^4.0.0",
    "bitcoin-address-validation": "^2.2.1",
    "bitcoinjs-lib": "^6.1.0",
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "web3": "^1.9.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn lint:js"
    ],
    "*.{css,scss}": [
      "yarn lint:css"
    ],
    "*.json": [
      "yarn lint:json"
    ]
  }
}
